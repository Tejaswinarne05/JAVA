class Animal 
{
    void makeSound() 
    {
        System.out.println("Sound.....");
    }
}
class Cat extends Animal 
{
    void makeSound() 
    {
        System.out.println("Meow.....");
    }
}
public class Main 
{
    public static void main(String[] args) 
    {
        Cat c = new Cat();
        c.makeSound();  
    }
}
