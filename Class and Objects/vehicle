class Vehicle
{
    void drive() 
    {
        System.out.println("Works.....");
    }
}
class Car extends Vehicle
{
    void drive() 
    {
        System.out.println("Repairing a car...");
    }
}
public class Main 
{
    public static void main(String[] args) 
    {
        Car c = new Car();
        c.drive();  
    }
}
